<div class="row justify-content-center">
  <div class="registration col-3 mt-5 mb-3 text-center">
    <form class="register-form col-14">
      <div class="form-group">
        <label for="exampleInputEmail1"></label>
        <input type="text" class="form-control" placeholder="Username" id="username-register" aria-describedby="emailHelp">
      </div>
      <div class="form-group">
        <label for="exampleInputPassword1"></label>
        <input type="text" class="form-control" placeholder="Email" id="email-register">
      </div>
      <div class="form-group">
        <label for="exampleInputPassword1"></label>
        <input type="password" class="form-control" placeholder="Password" id="password-register">
      </div>
      <div class="pb-3">
        <button type="submit" class="btn btn-outline-light">Register</button>
      </div>
    </form>
  </div>
</div>


